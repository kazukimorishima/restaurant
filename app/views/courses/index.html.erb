<div class="admin">
    <h1>コース写真一覧</h1>
    <hr>
    <% @courses.each do |c| %>
      <p><a href="/courses/<%= c.id %>/edit"><%= c.name %></a></p>
      <%= image_tag c.image.url(:thumb) %>
      <hr>
    <% end %>
    <a href="/admin"><button type="button" class="btn btn-success">トップに戻る</button></a>
</div>